SHOW\040DATABASES;
SHOW\040DATABASE\040MERTO;
SHOW\040DATABASE\040merto;
show\040users
;
show\040database
;
show\040databases;
SELECT\040user,authentication_string,plugin,host\040FROM\040mysql.user;
GRANT\040ALL\040PRIVILEGES\040ON\040merto.*\040TO\040'merto'@'localhost'\040WITH\040GRANT\040OPTION;
FLUSH\040PRIVILEGES;
SELECT\040user,authentication_string,plugin,host\040FROM\040mysql.user;
FLUSH\040PRIVILEGES;
SELECT\040user,authentication_string,plugin,host\040FROM\040mysql.user;
EXIT;
